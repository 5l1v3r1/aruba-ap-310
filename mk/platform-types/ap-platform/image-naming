IMAGE_NAME_SUFFIX_${MANUFACTURING_BUILD} := mfg

# order here is important; art must come after rd
IMAGE_NAME_SUFFIX_${REGULATORY_TESTING} := rd
IMAGE_NAME_SUFFIX_${ART_BUILD} := art
IMAGE_NAME_SUFFIX_${DEBUG_BUILD} := -debug
IMAGE_NAME_SUFFIX_${QDART_BUILD} := qdart

IMAGE_NAME := ${IMAGE_NAME_BASE}${IMAGE_NAME_SUFFIX_yes}.ari
